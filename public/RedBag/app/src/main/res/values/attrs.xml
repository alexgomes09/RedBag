<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="custom_font" format="string|reference"/>

    <declare-styleable name="CustomTextView">
        <attr name="custom_font"/>
        <attr name="left_edge_round" format="reference|dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="custom_font"/>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="custom_font"/>
        <attr name="hint" format="string|reference"/>
        <attr name="inputType" format="string"/>
        <attr name="label" format="string|reference"/>
        <attr name="maxLength" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BubbleFilterTextView">
        <attr name="bubble_radius" format="dimension"/>
    </declare-styleable>
</resources>